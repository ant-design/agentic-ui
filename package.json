{
  "name": "@ant-design/md-editor",
  "version": "1.14.10",
  "description": "类语雀的编辑器，支持 markdown 渲染和编辑",
  "repository": "git@github.com:ant-design/md-editor.git",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "doctor": "father doctor",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "linçt": "npm run lint:es && npm run lint:css",
    "prepare": "husky install && dumi setup",
    "prepublishOnly": "father doctor && npm run test && npm run build",
    "prettier": "prettier --write \"{src,test}/**/*.{js,jsx,ts,tsx,css,less,json,md}\"",
    "start": "npm run dev",
    "test": "vitest --run",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ]
  },
  "resolutions": {
    "d3-array": "2",
    "d3-color": "2",
    "d3-dsv": "2",
    "d3-force": "2",
    "d3-format": "2",
    "d3-geo": "2",
    "d3-hierarchy": "2",
    "d3-path": "2",
    "d3-scale-chromatic": "2",
    "d3-shape": "2"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.22.1",
    "@ant-design/icons": "^5.5.2",
    "@ant-design/pro-components": "^2.8.2",
    "@antv/g2": "^5.2.10",
    "@babel/runtime": "^7.26.0",
    "@juggle/resize-observer": "^3.4.0",
    "antd": "^5.22.7",
    "blob-util": "^2.0.2",
    "classnames": "^2.5.1",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.13",
    "diacritics": "^1.3.0",
    "direction": "^1.0.4",
    "dumi-theme-antd-style": "^0.31.1",
    "framer-motion": "^11.15.0",
    "glob": "^11.0.0",
    "is-hotkey": "^0.2.0",
    "is-plain-object": "^5.0.0",
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "mobx": "^6.13.5",
    "mobx-react": "^9.2.0",
    "motion": "^11.17.0",
    "nanoid": "^5.0.9",
    "partial-json-parser": "^1.2.2",
    "querystring": "^0.2.1",
    "rc-resize-observer": "^1.4.3",
    "react-resizable": "^3.0.5",
    "react-use": "^17.6.0",
    "remark": "^15.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "reveal.js": "^5.1.0",
    "rxjs": "^7.8.1",
    "scroll-into-view-if-needed": "^3.1.0",
    "shiki": "^1.24.4",
    "slash2": "^2.0.0",
    "slate": "0.112.0",
    "slate-dom": "^0.111.0",
    "slate-history": "0.110.3",
    "slate-hyperscript": "0.100.0",
    "string-width": "^7.2.0",
    "tiny-invariant": "1.3.1",
    "unified": "^11.0.5",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/classnames": "^2.3.4",
    "@types/diacritics": "^1.3.3",
    "@types/is-hotkey": "^0.1.10",
    "@types/jsdom": "^21.1.7",
    "@types/mdast": "^4.0.4",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-resizable": "^3.0.8",
    "@types/reveal.js": "^5.0.4",
    "@types/uuid": "^10.0.0",
    "@umijs/lint": "^4.4.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-istanbul": "^2.1.8",
    "dumi": "^2.4.17",
    "eslint": "^8.57.1",
    "father": "^4.5.1",
    "html2pdf.js": "^0.10.2",
    "husky": "^8.0.3",
    "jsdom": "^25.0.1",
    "lint-staged": "^13.3.0",
    "nwsapi": "2.2.13",
    "prettier": "^3.0.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stylelint": "^14.16.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "qixian.cs@outlook.com"
  ]
}
